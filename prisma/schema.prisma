datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

/// User model
model User {
  id       String  @id() @default(cuid())
  email    String  @unique()
  password String?
  entries  Entry[]
}

/// Post model
model Entry {
  id          String   @id() @default(cuid())
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt()
  title       String
  pathPrefix  String
  readingPath String
  entryType   String
  author      User     @relation(fields: [authorId], references: [id])
  authorId    String
}
