{
  "name": "docview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm run db-generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "style": "pnpm exec tailwindcss -i public/globals.css -o public/styles.css",
    "db": "npx zenstack generate && npx prisma db push",
    "db-generate": "npx zenstack generate",
    "db-migrate-prepare": "dotenv -e .env.local -- pnpm exec prisma migrate dev --create-only",
    "db-migrate": "dotenv -e .env.local -- pnpm exec prisma migrate dev"
  },
  "dependencies": {
    "@nextui-org/react": "^2.6.8",
    "@prisma/client": "6.1.x",
    "@relba/epubparser": "^1.0.2",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.39.3",
    "@tabler/icons-react": "^3.10.0",
    "@types/bun": "^1.1.14",
    "@vercel/blob": "^0.27.0",
    "@xmldom/xmldom": "^0.9.5",
    "@zenstackhq/runtime": "2.10.2",
    "@zenstackhq/server": "^2.10.2",
    "adm-zip": "^0.5.16",
    "cheerio": "^1.0.0",
    "epubjs": "^0.3.93",
    "es-toolkit": "^1.30.1",
    "framer-motion": "^11.0.25",
    "linkedom": "^0.18.6",
    "next": "15.1.0",
    "pdfjs-dist": "^4.1.392",
    "pspdfkit": "^2024.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.11.0",
    "@testing-library/react": "^15.0.7",
    "@types/adm-zip": "^0.5.7",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@zenstackhq/swr": "^2.10.2",
    "autoprefixer": "^10.4.19",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.1.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "prisma": "6.1.x",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5",
    "vitest": "^1.6.0",
    "wrangler": "^3.57.0",
    "zenstack": "2.10.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.1",
      "@types/react-dom": "19.0.2"
    }
  }
}
